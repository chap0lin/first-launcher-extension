{
    "manifest_version": 2,
    "name": "First Launcher Extension",
    "description": "First attempt of creating a launcher extension",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "background": {
        "scripts": ["./background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "notifications", "tabs", "storage", "webNavigation"]
}